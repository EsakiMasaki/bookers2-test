<% if user_signed_in? %>
  <%= form_with url: search_path , local: true , method: :get do |f| %>
    <%= f.text_field :word %>
    <%= f.select :range, options_for_select([["User"],["Book"]]) %>
    <%= f.select :search, options_for_select([["完全一致","1"],["前方一致","2"],["後方一致","3"],["部分一致","4"]]) %>
    <%= f.submit "検索" %>
  <% end %>
<% end %>